{
  "name": "kafka-client",
  "version": "1.1.3",
  "description": "Kafka client over lib kafka-node with stream interface & MessagePack codec",
  "main": "index.js",
  "dependencies": {
    "js-logger": "1.6.0",
    "kafka-node": "4.0.1",
    "msgpack5": "4.2.1",
    "signal": "github:spirinvladimir/signal#v1.1.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "mocha": "5.2.0",
    "nyc": "13.3.0"
  },
  "scripts": {
    "test": "nyc mocha -b --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "statements": 1,
    "branches": 1,
    "functions": 1,
    "lines": 1,
    "include": [
      "index.js"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ICMarkets/sql2k.git"
  },
  "keywords": [
    "mssql",
    "kafka",
    "stream"
  ],
  "author": "spirin.vladimir@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ICMarkets/sql2k/issues"
  },
  "homepage": "https://github.com/ICMarkets/sql2k#readme"
}
